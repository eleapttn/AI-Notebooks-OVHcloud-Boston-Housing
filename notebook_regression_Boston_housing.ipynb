   "source": [
    "### Step 4 - Split the dataset"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "1d6d515b",
   "metadata": {},
   "outputs": [],
   "source": [
    "# input of the neural network\n",
    "X = dataset.data\n",
    "\n",
    "# output of the neural network (only the price of houses)\n",
    "y = dataset.target\n",
    "\n",
    "# train and test split : 20 % for the test and 80 % for the learning\n",
    "X_train,X_test,y_train,y_test=train_test_split(X,y,test_size=0.2,random_state=0)\n",
    "\n",
    "# convert split data from numpy array to Pytorch tensors\n",
    "X_train = torch.FloatTensor(X_train)\n",
    "X_test = torch.FloatTensor(X_test)\n",
    "y_train = torch.FloatTensor(y_train)\n",
    "y_test = torch.FloatTensor(y_test)"
   ]
  },
